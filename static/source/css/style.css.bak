.navbar-inverse{
	background-color:#154660;
	border-color:#154660;
}
.navbar .navbar-brand{
	font-size: 20px;
	color: white;
}
.navbar-inverse .navbar-nav>li>a {
	color: white;
	font-size:18px;
}
.navbar-inverse .navbar-nav>li>a:hover{
	color: orange;
	font-size:18px;
}
.navbar{
	box-shadow: 1px 1px 5px black;
}
.body{
	background:white;
}
.table{
	width:800px;
	margin:auto;
	table-layout:fixed;
	text-align:center;
	margin-top:50px;
	font-family:Arial;
	color:#fff;
}
.td{
	border:1px dotted white;
	border-collapse:collapse;
	padding:50px;
	font-size:20px;
	background;purple;
}
.th{
	background:purple;
	padding:30px;
	text-transform:uppercase;
}